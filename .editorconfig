# This file is for unifying the coding style for different editors and IDEs
# editorconfig.org

root = true

[*]
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true
indent_size = 4
# Spaces in All types unless otherwise specified
indent_style = space

[Makefile]
indent_style = tab
